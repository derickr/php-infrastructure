# bless https://forum.ansible.com/t/ansible-playbook-through-proxy-with-interactive-login/11076/2
CanonicalizeHostname always
CanonicalDomains internal.php.net

Host jump-ams-1.internal.php.net jump-sfo-1.internal.php.net
    ProxyCommand   none
    AddKeysToAgent   no
    ForwardAgent    yes
    ControlMaster  auto
    ControlPersist   5d
    ControlPath    ~/.ssh/cp-socket-%C

Host *.internal.php.net
    ProxyJump jump-ams-1.internal.php.net
